                "category",
                "date",
                "type"
            ],
            "properties": {
                "amount": {
                    "type": "integer",
                    "minimum": 0
                },
                "category": {
                    "type": "string"
                },
                "date": {
                    "type": "string"
                },
                "type": {
                    "type": "string",
                    "enum": [
                        "доход",
                        "расход"
                    ]
                }
            }
        },
        "github_com_Komilov31_sales-tracker_internal_dto.ItemWithoutAggregated": {
            "type": "object",
            "properties": {
                "amount": {
                    "type": "integer"
                },
                "category": {
