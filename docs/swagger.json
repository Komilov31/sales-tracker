        "github_com_Komilov31_sales-tracker_internal_model.Item": {
            "type": "object",
            "properties": {
                "aggregated_data": {
                    "$ref": "#/definitions/github_com_Komilov31_sales-tracker_internal_model.Aggreagated"
                },
                "amount": {
