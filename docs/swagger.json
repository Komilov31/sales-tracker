{
    "swagger": "2.0",
    "info": {
        "description": "API for tracking sales, income, and expenses with analytics and CSV exports",
        "title": "Sales Tracker API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
